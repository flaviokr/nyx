<ul class="listar">
  <p><strong>Chamados criados por este t√©cnico: </strong></p>
  <% @user.chamados.each do |chamado| %>
    <li>
      <%= link_to "ID: #{chamado.id}", chamado %>
      | <%= link_to "editar", edit_chamado_path(chamado) %>
      <% if admin? %>
        | <%= link_to "deletar", chamado, method: :delete, data: { confirm: "Tem certeza que deseja deletar este chamado?" } %>
      <% end %>
    </li>
  <% end %>
</ul>